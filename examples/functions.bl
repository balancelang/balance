funcTakesIntReturnsInt(a: Int): Int {
    return a + 55
}

funcTakesNothingReturnsNothing() {
    print("Because this returns 'None', we can ommit ': None'")
}

var a = funcTakesIntReturnsInt(25)
print(a)
funcTakesNothingReturnsNothing()

functionWithAnyParameter(x) {
    print("Here, x implicitly has type 'Any' (x: Any)")
}
functionWithAnyParameter("Abc")