var cls = new MyClass()
cls.setA(25)
print(cls.getA())
// outputs 25


// We can reference the class both before and after its declaration
class MyClass {
    a: Int

    getA(): Int {
        return a
    }

    setA(value: Int): None {
        a = value
    }
}

// We can also initialize a class like this:
var myClass: MyClass = { a: 123 }
print(myClass.a)
// outputs 123